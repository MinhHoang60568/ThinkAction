<script type="text/javascript">
    jQuery(function ($) {
        function Counting(isRun) {
            if (isRun) {
                let valueDisplays = document.querySelectorAll(".logo-product-count");
                let interval = 5000;

                valueDisplays.forEach((valueDisplay) => {
                    let startValue = 0;
                    let endValue = parseInt($(valueDisplay).attr("data-val"));
                    let uraTime = Math.floor(interval / endValue);
                    let count = setInterval(function () {
                        startValue += 2;
                        valueDisplay.textContent = startValue + "+";
                        if (startValue == endValue) {
                            clearInterval(count);
                        }
                    }, uraTime);
                });
            }
        }
        var IsRun = true;
        $(window).scroll(function (event) {
            var pos_body = $('html,body').scrollTop();
            if (pos_body >= 800 && pos_body <= 1600) {
                Counting(IsRun);
                IsRun = false;
            }
        });
    });
</script>

