                $('#SERVICES_EVENTS_PC').html(data.find(x => x.Code === 'SERVICES_EVENTS').ValuePC.replace(/\n/g,'<br/>'));
                $('#SERVICES_EVENTS_MOBILE').html(data.find(x => x.Code === 'SERVICES_EVENTS').ValueMobile.replace(/\n/g,'<br/>'));
                $(".zalo-link").attr("href", data.find(x => x.Code === 'CONTACT_ZALO_LINK').ValuePC);
            });
    });
</script>

<script>
    function contactClickFunction(val) {
        jQuery(function ($) {
            var settings = {
              "url": "https://cms.thinkaction.vn/all-options",
              "method": "GET",
              "timeout": 0,
            };

            $.ajax(settings).done(function (response) {
                var data = response.data;
                if(val === "linkedin") {
                    window.open(data.find(x => x.Code === 'CONTACT_LINKEDIN_LINK').ValuePC);
                }
                if(val === "facebook") {
                    window.open(data.find(x => x.Code === 'CONTACT_FACEBOOK_LINK').ValuePC);
                }
                if(val === "zalo") {
                    window.open(data.find(x => x.Code === 'CONTACT_ZALO_LINK').ValuePC);
                }
                if(val === "youtube") {
                    window.open(data.find(x => x.Code === 'CONTACT_YOUTUBE_LINK').ValuePC);
                }
                if(val === "behance") {
                    window.open(data.find(x => x.Code === 'CONTACT_BEHANCE_LINK').ValuePC);
                }
            });
        });
    }
</script>