                            var imageLink = "https://cms.thinkaction.vn/" + value.Thumbnail;
                            var classId = "item-" + value.ProductCategoryId;
                             var html = '<div onclick="window.open(\'' + link + '\',\'_blank\');" style="cursor: pointer;"  class="c-archive__col c-archive__col--rounded o-xs-col-12 o-sm-col-6 o-md-col-6 o-lg-col-4 js-inf-scroll-item '+ classId+'">'+
                                            '<article class="c-entry  c-entry--tile c-entry--type-tile c-entry--status-publish c-entry--has-post-thumbnail c-entry--hentry u-text-left u-bgnd-base has-no-permalink has-header-image has-light-header-text s-color-scheme has-dark-header-bgnd"'+
                                                     'itemscope="itemscope" itemtype="https://schema.org/CreativeWork">'+
                                                '<div class="c-entry-header">'+
                                                    '<div style="" class="c-entry-header__image">'+
                                                        '<img src="' + imageLink + '" alt="" class="o-lazy-load o-lazy-load--img landscape cw-lesser featured full c-entry-header__img o-lazy-load--complete">'+
                                                        '<noscript>'+
                                                            '<img src="' + imageLink + '"'+
                                                                 'class="landscape cw-lesser featured full c-entry-header__img">'+
                                                        '</noscript>'+
                                                    '</div>'+
                                                    '<div class="c-tag c-entry-header__label u-z1">'+
                                                    '</div>'+
                                                '</div>'+
                                                '<div class="c-entry__content">'+
                                                    '<div class="c-entry__summary entry-summary s-cms-content"'+
                                                         'itemprop="description">'+
                                                        '<a href="'+link+'" target="_blank" class="o-link-color-on-hover"'+
                                                           'rel="noopener noreferrer">' +value.Name+ '</a>'+
                                                    '</div>'+
                                                '</div>'+
                                                '<div class="c-entry__anchor" id=""></div>'+
                                            '</article>'+
                                        '</div>';
                            $('#projects').append(html);
                            }
                        });
            });
    });
</script>

<script type="text/javascript">
    jQuery(function ($) {
        var settings = {
              "url": "https://cms.thinkaction.vn/all-options",
              "method": "GET",
              "timeout": 0,
            };

            $.ajax(settings).done(function (response) {
                var data = response.data;
                $('#SERVICES_DESIGN_PC').html(data.find(x => x.Code === 'SERVICES_DESIGN').ValuePC.replace(/\n/g,'<br/>'));
                $('#SERVICES_DESIGN_MOBILE').html(data.find(x => x.Code === 'SERVICES_DESIGN').ValueMobile.replace(/\n/g,'<br/>'));
                $(".zalo-link").attr("href", data.find(x => x.Code === 'CONTACT_ZALO_LINK').ValuePC);
            });
    });
</script>

<script>
    function contactClickFunction(val) {
        jQuery(function ($) {
            var settings = {
              "url": "https://cms.thinkaction.vn/all-options",
              "method": "GET",
              "timeout": 0,
            };

            $.ajax(settings).done(function (response) {
                var data = response.data;
                if(val === "linkedin") {
                    window.open(data.find(x => x.Code === 'CONTACT_LINKEDIN_LINK').ValuePC);
                }
                if(val === "facebook") {
                    window.open(data.find(x => x.Code === 'CONTACT_FACEBOOK_LINK').ValuePC);
                }
                if(val === "zalo") {
                    window.open(data.find(x => x.Code === 'CONTACT_ZALO_LINK').ValuePC);
                }
                if(val === "youtube") {
                    window.open(data.find(x => x.Code === 'CONTACT_YOUTUBE_LINK').ValuePC);
                }
                if(val === "behance") {
                    window.open(data.find(x => x.Code === 'CONTACT_BEHANCE_LINK').ValuePC);
                }
            });
        });
    }
</script>